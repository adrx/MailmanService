{
    "autoload": {
        "psr-4": {
            "Adrx\\MailmanService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adrx\\MailmanService\\Tests\\": "tests/"
        }
    },
    "description": "Allows the easy integration of Mailman into a dynamic website without using Python or requiring permission to Mailman binaries",
    "include-path": [
        "./"
    ],
    "license": "BSD License",
    "name": "adrx/mailman_service",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "ext-libxml": "*",
        "ext-dom": "*",
        "symfony/http-client": "^4.3",
        "symfony/browser-kit": "^4.3",
        "symfony/mime": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
